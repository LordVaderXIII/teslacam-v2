<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>teslacam-v2</Name>
  <Repository>lordvaderxiii/teslacam-v2-frontend:latest</Repository>
  <Registry>https://hub.docker.com/r/lordvaderxiii/teslacam-v2-frontend</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/LordVaderXIII/teslacam-v2</Support>
  <Project>https://github.com/LordVaderXIII/teslacam-v2</Project>
  <Overview>TeslaCam footage viewer.</Overview>
  <Category>MediaApp:Video</Category>
  <WebUI>http://[IP]:[PORT:80]</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/LordVaderXIII/teslacam-v2/main/unraid-template.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/LordVaderXIII/teslacam-v2/main/logo.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>2025-11-28</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Config Name="TeslaCam Path" Target="/usr/src/app/teslacam" Default="/mnt/user/teslacam" Mode="rw" Description="Path to TeslaCam footage" Type="Path" Display="always" Required="true" Mask="false"/>
  <Config Name="App Port" Target="80" Default="8099" Mode="tcp" Description="WebUI port" Type="Port" Display="always" Required="true" Mask="false"/>
  <Config Name="TESLACAM_DIR" Target="TESLACAM_DIR" Default="/mnt/user/teslacam" Mode="" Description="Env var for footage dir" Type="Variable" Display="always" Required="true" Mask="false"/>
</Container>
